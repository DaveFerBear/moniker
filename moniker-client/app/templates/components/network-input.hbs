<div class = "network-container col-sm-6 col-md-4 col-lg-3">
    <div class = "network panel panel-default">
        <div class="panel-heading">
            <h4 class = "panel-header">{{attr.name}}</h4>
        </div>
        <div class = "panel-body">
            <img src = {{attr.logo}}>
            <div id = "loaderContainer">
                <div class = "loader"></div>
            </div>
            <div id = "input">
                {{input value=inputVal key-up = "keyUp"}}
            </div>
        </div>
    </div>
</div>
{{yield}}
